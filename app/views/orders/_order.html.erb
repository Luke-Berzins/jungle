<% @order.line_items.each do |item| %>
 <tr>
  <td><%= image_tag item.product.image.tiny %></td>
    <td><%= item.product.name %><%= item.product.description %></td>
    <td><%= humanized_money_with_symbol item.product.price %></td>
    <td>
    <%= item.quantity %>
    </td>
    <td>
    <%= humanized_money_with_symbol item.product.price * item.quantity %>
    </td>
  </tr>
<% end %>